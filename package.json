{
  "name": "netlify-cms-widget-inline-select",
  "version": "1.3.3",
  "description": "Widget for radios and inline multi-checkboxes in Netlify CMS.",
  "keywords": [
    "checkbox",
    "cms",
    "inline",
    "multi",
    "netlify",
    "netlify-cms",
    "radio",
    "widget"
  ],
  "homepage": "https://github.com/karolis-sh/netlify-cms-widget-inline-select#readme",
  "bugs": {
    "url": "https://github.com/karolis-sh/netlify-cms-widget-inline-select/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karolis-sh/netlify-cms-widget-inline-select.git"
  },
  "license": "MIT",
  "author": "Karolis Å arapnickis",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:demo": "webpack --devtool source-map --config webpack.dev.config.js",
    "build:package": "babel src --out-dir lib --source-maps --ignore *.test.js",
    "clean": "rm -rf public",
    "commit": "npx cz",
    "dev": "webpack serve --config webpack.dev.config.js",
    "format": "run-p format:*",
    "format:package.json": "sort-package-json package.json",
    "format:prettier": "prettier --write '**/*.*'",
    "lint": "run-p lint:*",
    "lint:js": "eslint .",
    "lint:md": "markdownlint-cli2 '**/*.md' '!node_modules' '!**/node_modules' '!**/CHANGELOG.md'",
    "qa": "run-p lint test",
    "prerelease": "git checkout master && git pull && yarn",
    "release": "run-s clean build qa && standard-version --no-verify && git push --follow-tags origin master --no-verify && yarn publish",
    "test": "jest",
    "try": "run-s clean build:demo && serve public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn qa"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{md,json,yml}": [
      "pretty-quick --staged"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "netlify-cms-ui-default": "^2.11.6",
    "prop-types": "^15.7.2",
    "react-immutable-proptypes": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@k.sh/eslint-config": "^1.18.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "immutable": "^4.0.0-rc.12",
    "jest": "^26.5.3",
    "lint-staged": "^10.4.0",
    "markdownlint-cli2": "^0.0.10",
    "netlify-cms": "^2.10.63",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "serve": "^11.3.2",
    "sort-package-json": "^1.46.0",
    "source-map-loader": "^1.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "netlify-cms": ">2.8",
    "react": "^16"
  }
}
